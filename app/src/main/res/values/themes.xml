<resources>
	<!-- Base application theme. -->
	<style name="Theme.DinDinTest" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
		<!-- Primary brand color. -->
		<item name="colorPrimary">@color/purple_500</item>
		<item name="colorPrimaryVariant">@color/purple_700</item>
		<item name="colorOnPrimary">@color/white</item>
		<!-- Secondary brand color. -->
		<item name="colorSecondary">@color/teal_200</item>
		<item name="colorSecondaryVariant">@color/teal_700</item>
		<item name="colorOnSecondary">@color/black</item>
		<!-- Customize your theme here. -->
	</style>

	<style name="Theme.DinDinTest.NoActionBar">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
	</style>

	<style name="Widget.MaterialComponents.Chip.Filter.Outlined" parent="Widget.MaterialComponents.Chip.Filter">
		<item name="chipBackgroundColor">@color/chip_outlined_bg_color</item>
		<item name="chipStrokeColor">@color/chip_outlined_stroke_color</item>
		<item name="chipStrokeWidth">1dp</item>
	</style>

	<style name="Theme.DinDinTest.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

	<style name="Theme.DinDinTest.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>